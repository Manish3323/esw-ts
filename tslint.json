{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "defaultSeverity": "error",
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-consecutive-blank-lines": false,
    "no-console": false
  }
}

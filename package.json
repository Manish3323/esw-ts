{
  "name": "gateway-tsclient",
  "version": "1.0.0",
  "description": "Typescript client for gateway",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "audit": "npm audit",
    "clean": "rm -rf dist && mkdir dist",
    "prebuild": "npm-run-all --parallel audit clean fix",
    "build": "tsc",
    "build:watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "fix": "npm-run-all --parallel fix:lint fix:prettier",
    "fix:lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix:prettier": "prettier --write --config ./.prettierrc './src/**/*.ts'"
  },
  "scripts-info": {
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.3.0",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "ts-jest": "^25.3.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "whatwg-fetch": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmtsoftware/gateway-tsclient.git"
  },
  "keywords": [
    "typescript",
    "gateway"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tmtsoftware/gateway-tsclient/issues"
  },
  "homepage": "https://github.com/tmtsoftware/gateway-tsclient#readme"
}
